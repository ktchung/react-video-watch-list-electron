{
  "name": "watch-list-electron",
  "homepage": ".",
  "version": "0.1.4",
  "private": true,
  "dependencies": {
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@mui/icons-material": "^5.11.0",
    "@mui/lab": "^5.0.0-alpha.118",
    "@mui/material": "^5.11.7",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^13.0.0",
    "@testing-library/user-event": "^13.2.1",
    "@types/jest": "^27.0.1",
    "@types/node": "^16.7.13",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "axios": "^1.3.2",
    "lodash-es": "^4.17.21",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.43.0",
    "react-scripts": "5.0.1",
    "react-uuid": "^2.0.0",
    "typescript": "*",
    "web-vitals": "^2.1.0"
  },
  "scripts": {
    "dev": "react-scripts start",
    "start": "serve ./build",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "release": "standard-version",
    "release:dry": "standard-version --dry-run",
    "electron:dev": "concurrently \"yarn dev\" \"wait-on http://localhost:3001 && electron .\"",
    "electron:tsc": "tsc --p ./electron",
    "electron:start": "electron:tsc && concurrently \"yarn build && yarn start\" \"wait-on http://localhost:3000 && electron:launch\"",
    "electron:launch": "electron ./electron/app.js"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "main": "electron/main.ts",
  "license": "MIT",
  "devDependencies": {
    "@types/electron": "^1.6.10",
    "@types/lodash-es": "^4.17.7",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "concurrently": "^8.0.1",
    "electron": "^24.1.3",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.6.0",
    "eslint-config-standard-with-typescript": "^34.0.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.32.2",
    "gh-pages": "^5.0.0",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.3",
    "serve": "^14.2.0",
    "standard-version": "^9.5.0",
    "wait-on": "^7.0.1"
  }
}
